[decl void:putchar [char:c]]
[decl int:rand [void]]
[decl void:srand [int:i]]
[decl int:time [void*:p]]

[defn int:main [void]
	[srand [time null]]
	[let int:comp_guess [% [rand] 100]]
	[putn comp_guess]
	[while 1
		[putn 12]]]

[defn void:putn [int:n]
	[if [> n 0]
		[let int:a [/ n 10]]
		[set n [- n [* 10 a]]]
		[putn a]]
	[putchar [+ '0' n]]]
