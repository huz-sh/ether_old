[let int:hello]
[let int:c]
[let char*:str]

[define int:main [void]
	[let int:var 2]
	[console_create 1 2]
	[set var [* 2 3]]
	
	[if [= var 3]
		[set var 3]]
	[elif [= c 3]
		[set c 10]]
	[else
		[set c 2]]
	[if 1
		[if 2
			[+ 3 3 2]]]]

[define console:console_create [int:x int:y]
	[set c [linux_console_create x y]]]

[struct console
	[uint:x]
	[uint:y]
	[platform_data:data]]

[struct uint]
[struct u32]

[struct platform_data
	[win_platform_data:data]
	[platform_data*:c]]

[struct win_platform_data
	[int:x]]

[define int:linux_console_create [int:x int:y]
    [set hello [+ 4 3 5 4]]]
