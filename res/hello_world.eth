;; note that Ether's semantics are not finalized, and can be 
;; changed during development. so do not adhere to this syntax.
[deploy io] ;; includes   

[namespace ether
	[namespace io
		[void log_error [string s]
			[std::io::printfl "error: %" s]
			[std::sys::exit 1.0]]
		
		[void log_warning [string s]
			[std::io::printfl "warning: %" s]]]]

[int: main [void]
	[ether::io::log_error "Hello, world!"]]